                                            $revenue = $assignmentprofitandloss->engagementfee ?? 0;
                                            $cost = $assignmentprofitandloss->total_cost ?? 0;
                                            $profit = $revenue - $cost;
                                            $margin = $revenue != 0 ? ($profit / $revenue) * 100 : 0;
                                        @endphp
