      //   ->groupBy('assignmentmappings.assignmentgenerate_id')
      //   ->get()
      //   ->sum('total_not_filled');


      $teamPending = DB::table('assignmentmappings')
        ->leftJoin('assignmentbudgetings', 'assignmentbudgetings.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
        ->leftJoin('assignmentteammappings', 'assignmentteammappings.assignmentmapping_id', '=', 'assignmentmappings.id')
        ->leftJoin('teammembers', 'teammembers.id', '=', 'assignmentteammappings.teammember_id')
        ->leftJoin('annual_independence_declarations', function ($join) {
          $join->on('annual_independence_declarations.assignmentgenerateid', '=', 'assignmentmappings.assignmentgenerate_id')
            ->on('annual_independence_declarations.createdby', '=', 'teammembers.id');
          // ->where('annual_independence_declarations.type', 2);
        })
        ->leftJoin('independences', function ($join) {
          $join->on('independences.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
            ->on('independences.createdby', '=', 'teammembers.id');
        })
        ->whereBetween(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), [$startDateFormatted, $endDateFormatted])
        ->when(!empty($monthsdigit), function ($query) use ($monthsdigit) {
          $query->whereMonth(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), $monthsdigit);
        })
        ->when(!empty($partnerId), function ($query) use ($partnerId) {
          $query->where('assignmentmappings.leadpartner', $partnerId);
        })
        ->whereNull(DB::raw('COALESCE(annual_independence_declarations.id, independences.id)')) // Members without declarations
        ->groupBy('assignmentmappings.assignmentgenerate_id')
        ->select(DB::raw('COUNT(*) as total_not_filled'))
        ->get()
        ->sum('total_not_filled');


      $otherPartnerPending = DB::table('assignmentmappings')
        ->leftJoin('assignmentbudgetings', 'assignmentbudgetings.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
        ->leftJoin('teammembers', 'teammembers.id', '=', 'assignmentmappings.otherpartner')
        ->leftJoin('annual_independence_declarations', function ($join) {
          $join->on('annual_independence_declarations.assignmentgenerateid', '=', 'assignmentmappings.assignmentgenerate_id')
            ->on('annual_independence_declarations.createdby', '=', 'assignmentmappings.otherpartner');
        })
        ->leftJoin('independences', function ($join) {
          $join->on('independences.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
            ->on('independences.createdby', '=', 'assignmentmappings.otherpartner');
        })
        ->whereBetween(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), [$startDateFormatted, $endDateFormatted])
        ->when(!empty($monthsdigit), function ($query) use ($monthsdigit) {
          $query->whereMonth(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), $monthsdigit);
        })
        ->when(!empty($partnerId), function ($query) use ($partnerId) {
          $query->where('assignmentmappings.leadpartner', $partnerId);
        })
        ->whereNotNull('assignmentmappings.otherpartner')
        ->whereNull(DB::raw('COALESCE(annual_independence_declarations.id, independences.id)'))
        ->count();

      $eqcrPartnerPending = DB::table('assignmentmappings')
        ->leftJoin('assignmentbudgetings', 'assignmentbudgetings.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
        ->leftJoin('teammembers', 'teammembers.id', '=', 'assignmentmappings.eqcrpartner')
        ->leftJoin('annual_independence_declarations', function ($join) {
          $join->on('annual_independence_declarations.assignmentgenerateid', '=', 'assignmentmappings.assignmentgenerate_id')
            ->on('annual_independence_declarations.createdby', '=', 'assignmentmappings.eqcrpartner');
        })
        ->leftJoin('independences', function ($join) {
          $join->on('independences.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
            ->on('independences.createdby', '=', 'assignmentmappings.eqcrpartner');
        })
        ->whereBetween(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), [$startDateFormatted, $endDateFormatted])
        ->when(!empty($monthsdigit), function ($query) use ($monthsdigit) {
          $query->whereMonth(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), $monthsdigit);
        })
        ->when(!empty($partnerId), function ($query) use ($partnerId) {
          $query->where('assignmentmappings.leadpartner', $partnerId);
        })
        ->whereNotNull('assignmentmappings.eqcrpartner')
        ->whereNull(DB::raw('COALESCE(annual_independence_declarations.id, independences.id)'))
        ->count();


      $totalNotFilled = $teamPending + $otherPartnerPending + $eqcrPartnerPending;


22222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222
dashboard controller 


    public function İndependencepending(Request $request)
    {
        $filtersdata = $this->filtersRequest($request);

        $yearly = $filtersdata['yearly'];
        $monthsdigit = $filtersdata['monthsdigit'];
        $partnerId = $filtersdata['partnerId'];
        $startDateFormatted = $filtersdata['startDateFormatted'];
        $endDateFormatted = $filtersdata['endDateFormatted'];
        $months = $filtersdata['months'];

        // Independence Acceptance Pending
        $teamPending = DB::table('assignmentmappings')
            ->leftJoin('assignmentbudgetings', 'assignmentbudgetings.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
            ->leftJoin('assignmentteammappings', 'assignmentteammappings.assignmentmapping_id', '=', 'assignmentmappings.id')
            ->leftJoin('teammembers', 'teammembers.id', '=', 'assignmentteammappings.teammember_id')
            ->leftJoin('teammembers as partner', 'partner.id', '=', 'assignmentmappings.leadpartner')
            ->leftJoin('clients', 'clients.id', '=', 'assignmentbudgetings.client_id')
            ->leftJoin('assignments', 'assignments.id', '=', 'assignmentmappings.assignment_id')
            ->leftJoin('annual_independence_declarations', function ($join) {
                $join->on('annual_independence_declarations.assignmentgenerateid', '=', 'assignmentmappings.assignmentgenerate_id')
                    ->on('annual_independence_declarations.createdby', '=', 'teammembers.id');
            })
            ->leftJoin('independences', function ($join) {
                $join->on('independences.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
                    ->on('independences.createdby', '=', 'teammembers.id');
            })
            ->when(!empty($yearly), function ($query) use ($startDateFormatted, $endDateFormatted) {
                $query->whereBetween(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), [$startDateFormatted, $endDateFormatted]);
            })
            ->when(!empty($monthsdigit), function ($query) use ($monthsdigit) {
                $query->whereMonth(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), $monthsdigit);
            })
            ->when(!empty($partnerId), function ($query) use ($partnerId) {
                $query->where('assignmentmappings.leadpartner', $partnerId);
            })
            ->whereNull(DB::raw('COALESCE(annual_independence_declarations.id, independences.id)'))
            ->select(
                'assignmentmappings.id as mapping_id',
                'assignmentmappings.assignmentgenerate_id',
                'assignmentmappings.created_at',
                'assignmentbudgetings.assignmentname',
                'clients.client_name',
                'assignments.assignment_name',
                'teammembers.team_member as team_member_name',
                'partner.team_member as partnername'
            )
            ->get();


        $otherPartnerPending = DB::table('assignmentmappings')
            ->leftJoin('assignmentbudgetings', 'assignmentbudgetings.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
            ->leftJoin('teammembers', 'teammembers.id', '=', 'assignmentmappings.otherpartner')
            ->leftJoin('teammembers as partner', 'partner.id', '=', 'assignmentmappings.leadpartner')
            ->leftJoin('clients', 'clients.id', '=', 'assignmentbudgetings.client_id')
            ->leftJoin('assignments', 'assignments.id', '=', 'assignmentmappings.assignment_id')
            ->leftJoin('annual_independence_declarations', function ($join) {
                $join->on('annual_independence_declarations.assignmentgenerateid', '=', 'assignmentmappings.assignmentgenerate_id')
                    ->on('annual_independence_declarations.createdby', '=', 'assignmentmappings.otherpartner');
            })
            ->leftJoin('independences', function ($join) {
                $join->on('independences.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
                    ->on('independences.createdby', '=', 'assignmentmappings.otherpartner');
            })
            ->when(!empty($yearly), function ($query) use ($startDateFormatted, $endDateFormatted) {
                $query->whereBetween(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), [$startDateFormatted, $endDateFormatted]);
            })
            ->when(!empty($monthsdigit), function ($query) use ($monthsdigit) {
                $query->whereMonth(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), $monthsdigit);
            })
            ->when(!empty($partnerId), function ($query) use ($partnerId) {
                $query->where('assignmentmappings.leadpartner', $partnerId);
            })
            ->whereNotNull('assignmentmappings.otherpartner')
            ->whereNull(DB::raw('COALESCE(annual_independence_declarations.id, independences.id)'))
            ->select(
                'assignmentmappings.id as mapping_id',
                'assignmentmappings.assignmentgenerate_id',
                'assignmentmappings.created_at',
                'assignmentbudgetings.assignmentname',
                'clients.client_name',
                'assignments.assignment_name',
                'teammembers.team_member as team_member_name',
                'partner.team_member as partnername'
            )
            ->get();

        $eqcrPartnerPending = DB::table('assignmentmappings')
            ->leftJoin('assignmentbudgetings', 'assignmentbudgetings.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
            ->leftJoin('teammembers', 'teammembers.id', '=', 'assignmentmappings.eqcrpartner')
            ->leftJoin('teammembers as partner', 'partner.id', '=', 'assignmentmappings.leadpartner')
            ->leftJoin('clients', 'clients.id', '=', 'assignmentbudgetings.client_id')
            ->leftJoin('assignments', 'assignments.id', '=', 'assignmentmappings.assignment_id')
            ->leftJoin('annual_independence_declarations', function ($join) {
                $join->on('annual_independence_declarations.assignmentgenerateid', '=', 'assignmentmappings.assignmentgenerate_id')
                    ->on('annual_independence_declarations.createdby', '=', 'assignmentmappings.eqcrpartner');
            })
            ->leftJoin('independences', function ($join) {
                $join->on('independences.assignmentgenerate_id', '=', 'assignmentmappings.assignmentgenerate_id')
                    ->on('independences.createdby', '=', 'assignmentmappings.eqcrpartner');
            })
            ->when(!empty($yearly), function ($query) use ($startDateFormatted, $endDateFormatted) {
                $query->whereBetween(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), [$startDateFormatted, $endDateFormatted]);
            })
            ->when(!empty($monthsdigit), function ($query) use ($monthsdigit) {
                $query->whereMonth(DB::raw('COALESCE(assignmentbudgetings.actualstartdate, assignmentbudgetings.tentativestartdate)'), $monthsdigit);
            })
            ->when(!empty($partnerId), function ($query) use ($partnerId) {
                $query->where('assignmentmappings.leadpartner', $partnerId);
            })
            ->whereNotNull('assignmentmappings.eqcrpartner')
            ->whereNull(DB::raw('COALESCE(annual_independence_declarations.id, independences.id)'))
            ->select(
                'assignmentmappings.id as mapping_id',
                'assignmentmappings.assignmentgenerate_id',
                'assignmentmappings.created_at',
                'assignmentbudgetings.assignmentname',
                'clients.client_name',
                'assignments.assignment_name',
                'teammembers.team_member as team_member_name',
                'partner.team_member as partnername'
            )
            ->get();

        $clientspecificindependencedeclaration = $teamPending->merge($otherPartnerPending)->merge($eqcrPartnerPending);

        return view('backEnd.dashboardreport.independencepending', compact('clientspecificindependencedeclaration'));
    }


