                        $msg->cc($data['assignby']);
                        $msg->subject('kgs OMS Ticket New Instruction');
                    });
                }
            } else {
                if ($request->status == 1) {
                    $data = array(
                        'taskname' =>  $tasks->taskname,
                        'description' =>  $request->remark,
                        'assignby'  => $assignby->emailid,
                        'assignbyname'  => $assignby->team_member,
                        'inform_partner'  => $tasks->inform_partner,
                        'partner_id'  => $tasks->partner_id,
                    );
                    $data['ccmail'] = Teammember::where('id', $tasks->partner_id)->first()->emailid;

                    Mail::send('emails.omstaskdone', $data, function ($msg) use ($data) {
                        $msg->to('simmi@capitall.io');
                        if ($data['partner_id'] && $data['inform_partner'] == 1) {
                            $msg->cc($data['ccmail']);
                        }
                        $msg->subject('kgs OMS Ticket Completed');
                    });
                } else {
                    $data = array(
                        'taskname' =>  $tasks->taskname,
                        'description' =>  $request->remark,
                        'assignby'  => $assignby->emailid,
                        'assignbyname'  => $assignby->team_member,
                        'inform_partner'  => $tasks->inform_partner,
                        'partner_id'  => $tasks->partner_id,
                    );
                    $data['ccmail'] = Teammember::where('id', $tasks->partner_id)->first()->emailid;

                    Mail::send('emails.omstaskinstruction', $data, function ($msg) use ($data) {
                        $msg->to('simmi@capitall.io');
                        if ($data['partner_id'] && $data['inform_partner'] == 1) {
                            $msg->cc($data['ccmail']);
                        }
                        $msg->subject('kgs OMS Ticket New Instruction');
                    });
                }
            }

            $output = array('msg' => 'Submit Successfully');

			



